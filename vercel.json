{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/api-proxy/(.*)", "dest": "https://generativelanguage.googleapis.com/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "trailingSlash": false
}
